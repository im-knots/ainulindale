{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Ainulindale default permissions - shell execution, filesystem access, dialogs",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-execute",
    "shell:allow-spawn",
    "shell:allow-stdin-write",
    "shell:allow-kill",
    "dialog:allow-open"
  ]
}
